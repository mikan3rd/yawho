{
  "private": true,
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "engines": {
    "node": "14"
  },
  "scripts": {
    "frontend": "yarn workspace frontend",
    "backend": "yarn workspace backend",
    "lint": "eslint .",
    "lint:frontend": "eslint ./packages/frontend",
    "lint:backend": "eslint ./packages/backend",
    "codegen": "yarn frontend codegen && yarn eslint ./packages/frontend/graphql --fix"
  },
  "dependencies": {
    "@apollo/client": "3.3.21",
    "@prisma/client": "2.27.0",
    "@sentry/node": "6.9.0",
    "@sentry/react": "6.9.0",
    "@sentry/tracing": "6.9.0",
    "@sentry/webpack-plugin": "1.16.0",
    "dayjs": "1.10.6",
    "firebase": "8.7.1",
    "firebase-admin": "9.11.0",
    "graphql": "15.5.1"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.2.0",
    "@types/node": "14.17.5",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "dotenv": "8.6.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "globby": "11.0.4",
    "prettier": "2.3.2",
    "prisma": "2.27.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
